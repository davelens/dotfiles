{
  "title": "Firefox: Hold 0–9 for tab navigation",
  "rules": [
    {
      "description": "Firefox: Hold 0–9 for tab navigation",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "1", "modifiers": { "optional": ["any"] } },
          "to_if_held_down": [
            { "key_code": "1", "modifiers": ["left_command"], "halt": true }
          ],
          "to_after_key_up": [
            { "key_code": "1" }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": ["^org\\.mozilla\\.firefox$"]
            }
          ]
        },
